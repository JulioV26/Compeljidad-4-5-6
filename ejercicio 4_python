def aSaber(v,tamano):
n = 0  
resultado = 0; n = n+1
i = 0; n = n+1
while i < tamano :
resultado = resultado + v[i]; n = n+1
i = i + 1;n = n+1
print(f"La complejidad del algoritmo es:",n)
return resultado
arreglo = [5,10,15,20]
final = aSaber(arreglo,len(arreglo))
print("La suma de los elementos es:", final)
