def aSaber(v,tamano):
n = 0
resultado = 0; n = n+1
i = tamano - 1; n = n+1
while i >= 0:
resultado = resultado + v[i]; n = n+1
i = i - 1; n = n+1
print(f"La complejidad del algoritmo es: {n}")
return resultado
arreglo = [2,5,7,9]
final = aSaber(arreglo,len(arreglo))
print(f"La suma de los elementos es: {final}")
